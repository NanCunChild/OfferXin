<view class="funcCard">
    <view class="funcInnerCard">
        <view style="margin: 20rpx 5rpx 2rpx 30rpx; font-size: 36rpx;">常用功能</view>
        <view class="btnFucInRow">
            <view class="otherfuncImgContainer" bindtap="go" data-url="https://mp.weixin.qq.com/s/6bFGEKjtEd1MSLn-OKEJug">
                <image src="/images/mine/qq.png" class="funcImg"></image>
                <view style="font-size: 22rpx;">加入社群</view>
            </view>
            <view class="otherfuncImgContainer" bindtap="go" data-url="https://mp.weixin.qq.com/s/_i6AMGo0OjMtrcne-xzMbA">
                <image src="/images/mine/commend.png" class="funcImg"></image>
                <view style="font-size: 22rpx;">名企内推</view>
            </view>
            <view class="funcImgContainer">
                <image src="/images/mine/feedback.png" class="funcImg"></image>
                <view style="font-size: 22rpx;">问题反馈</view>
                <button open-type="feedback" id="feedbackBtn"></button>
            </view>
            <view class="otherfuncImgContainer" bindtap="showShare">
                <image src="/images/mine/share.png" class="funcImg"></image>
                <view style="font-size: 22rpx;">好用分享</view>
            </view>
        </view>

        <view class="btnFucInRow">
            <view class="otherfuncImgContainer" bindtap="go" data-url="https://mp.weixin.qq.com/s/6bFGEKjtEd1MSLn-OKEJug">
                <image src="/images/mine/qq.png" class="funcImg"></image>
                <view style="font-size: 22rpx;">加入社群</view>
            </view>
            <view class="otherfuncImgContainer" bindtap="go" data-url="https://mp.weixin.qq.com/s/_i6AMGo0OjMtrcne-xzMbA">
                <image src="/images/mine/commend.png" class="funcImg"></image>
                <view style="font-size: 22rpx;">名企内推</view>
            </view>
            <view class="funcImgContainer">
                <image src="/images/mine/feedback.png" class="funcImg"></image>
                <view style="font-size: 22rpx;">问题反馈</view>
                <button open-type="feedback" id="feedbackBtn"></button>
            </view>
            <view class="otherfuncImgContainer" bindtap="showShare">
                <image src="/images/mine/share.png" class="funcImg"></image>
                <view style="font-size: 22rpx;">好用分享</view>
            </view>
        </view>
    </view>
</view>


