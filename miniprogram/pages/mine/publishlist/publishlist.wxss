  text-align: center;
  font-weight: 550;
  font-size: 30rpx;
  color: #7e6f6f;
}
.divide_line{
  margin: 0 8%;
  border-bottom: 1rpx solid #e2dddd;
}
.tabbar{
  opacity: 0.9;
}