}
.left{
  width: 45%;
  text-align: left;
  justify-content: left;
}
.right{
  width: 55%;
  text-align: right;
  justify-content: right;
}

.search_box{
  width: 100%;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
