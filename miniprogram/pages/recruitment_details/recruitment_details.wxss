/* pages/recruitment_details/recruitment_details.wxss */
page {
  display: flex;
  justify-content: center;
  background-color: #f0f0f0;
  /* background-color: #d4d4d4; */
}

.row_container {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.r_box {
  width: 100%;
  background: rgb(255, 255, 255);
  opacity: 1;
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
  padding: 24rpx;
  border-top: 1rpx solid #eee;
  align-items: center;
  z-index: 8;
  display: flex;
  justify-content: left;
}

.r_field {
  width: 80%;
  background-color: rgb(235, 235, 235);
  border-radius: 25rpx;
  padding: 16rpx;
}

.rb_box {
  margin-left: 3%;
}

.r_button {
  font-size: 24rpx;
  font-weight: 600;
  color: #7e6f6f;
  background: transparent;
  border-color: transparent;
  padding: 0;
}

.top_box {
  position: relative;
  width: 100%;
  height: 500rpx;
  padding: 30rpx;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}
.top_box:after {
  width: 140%;
  height: 500rpx;
  position: absolute;
  left: -20%;
  top: 0;
  z-index: -1;
  content: "";
  border-radius: 0 0 50% 50%;
  background: #4169e1;
}

